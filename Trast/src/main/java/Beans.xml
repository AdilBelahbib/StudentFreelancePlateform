<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="utilisateur" class="com.trast.model.Utilisateur">
		<property name="email" />
		<property name="motDePasse" />
		<property name="fichiers"> <set></set></property>
		<property name="adresses"> <set></set></property>
	</bean>

	<bean id="particulier" parent="Utilisateur">
		<property name="nomUtilisateur" />
		<property name="nom" />
		<property name="prenom" />
		<property name="dateNaissance" />
	</bean>

	<bean id="entreprise" parent="Utilisateur">
		<property name="sigle" />
		<property name="secteurActivites"> 
			<set>
			</set>
		</property>
		<property name="apercu" />
		<property name="appelOffres"> 
			<set>
			</set>
		</property>
	</bean>

	<bean id="etudiant" parent="Particulier">
		<property name="nombreBids" />
		<property name="qualifications"> 
			<set>
			</set>
		</property>
		<property name="competences"> 
			<set>
			</set>
		</property>
		<property name="cahiersDesCharges"> 
			<set>
			</set>
		</property>
		<property name="contrePropositions"> 
			<set>
			</set>
		</property>
	</bean>

	<bean id="adresse" class="com.trast.model.Adresse">
		<property name="ville" />
		<property name="pays" />
		<property name="adresse" />
		<property name="utilisateur" ref="utilisateur" />
	</bean>

	<bean id="fichier" class="com.trast.model.Fichier">
		<property name="titre" />
		<property name="chemin" />
		<property name="cahierDesCharges" ref="cahierDesCharges" />
		<property name="utilisateur" ref="utilisateur" />
		<property name="contreProposition" ref="contreProposition" />
	</bean>

	<bean id="contreProposition" class="com.trast.model.ContreProposition">
		<property name="enchere" />
		<property name="fichier" ref="fichier" />
		<property name="appelOffre" ref="appelOffre" />
		<property name="etudiant" ref="etudiant" />
	</bean>

	<bean id="cahierDesCharges" class="com.trast.model.CahierDesCharges">
		<property name="sujet" />
		<property name="dateDebut" ref="fichier" />
		<property name="dateFin" ref="appelOffre" />
		<property name="fichiers">
		 	<set>
		 	</set>
		</property>
		<property name="etudiant" ref="etudiant" />
		<property name="appelOffre" ref="appelOffre" />
	</bean>

	<bean id="appelOffre" class="com.trast.model.AppelOffre">
		<property name="dateExpiration" />
		<property name="dateDebut" />
		<property name="remunerations">
		 	<set>
		 	</set>
		</property>
		<property name="qualifications"> 
			<set>
			</set>
		</property>
		<property name="etudiants"> 
			<set>
			</set>
		</property>
		<property name="entreprise" ref="entreprise" />
		<property name="cahierDesCharges" ref="cahierDesCharges" />
	</bean>

	<!-- bean: Etablissement -->

	<bean id="etablissement" class="com.trast.model.Etablissement">
		<property name="id" />
		<property name="nomEtablissement" />
		<property name="intituleFormations">
			<set>
			</set>
		</property>
	</bean>
	<!-- bean: IntituleFormation -->
	<bean id="intituleFormation" class="com.trast.model.IntituleFormation">
		<property name="id"  />
		<property name="intitule" />
		<property name="etablissement" ref="etablissement" />
		<property name="formations">
			<set>
			</set>
		</property>
	</bean>
	<!-- bean: Remuneration -->
	<bean id="remuneration" class="com.trast.model.Remuneration">
		<property name="id"  />
		<property name="somme" />
		<property name="intitule"  />
		<property name="devise"  />
		<property name="appelOffre" ref="appelOffre" />
	</bean>
	<!-- bean: Qualification -->
	<bean id="qualification" class="com.trast.model.Qualification">
		<property name="id"  />
		<property name="dateDebut"/>
		<property name="dateFin" />
		<property name="etudiant" ref="etudiant" />
		<property name="fichiers">
			<set>
			</set>
		</property>
		<property name="appelOffre">
		</property>
	</bean>
	<!-- bean: Formation -->
	<bean id="formation" class="com.trast.model.Formation" parent="qualification">
		<property name="intituleFormation" ref="intituleFormation" />
	</bean>
	<!-- bean: Experience -->
	<bean id="experience" class="com.trast.model.Experience" parent="qualification">
		<property name="responsabilite"  />
		<property name="typeExperience"  />
		<property name="structureAccueil"  />
		<property name="sujet"  />
		<property name="salaire"  />
	</bean>
	<!-- bean: Competence -->
	<bean id="competence" class="com.trast.model.Competence">
		<property name="id" />
		<property name="intitule"  />
		<property name="niveau" />
		<property name="etudiant" ref="etudiant" />
	</bean>
	<!-- bean: Projet -->
	<bean id="projet" class="com.trast.model.Projet">
		<property name="id" />
		<property name="etudiant" ref="etudiant" />
		<property name="cahierDesCharges" ref="cahierDesCharges" />
	</bean>
	<!-- bean: Administrateur -->
	<bean id="administrateur" class="com.trast.model.Administrateur"
		parent="particulier">
	</bean>
</beans>